{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/index.ts"
    },
    {
      "src": "/api/contact-submissions", 
      "dest": "/api/index.ts"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "buildCommand": "./build",
  "framework": null,
  "env": {
    "DATABASE_URL": "@database_url"
  }
}