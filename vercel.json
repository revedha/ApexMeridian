{
  "version": 2,
  "functions": {
    "dist/index.js": {}
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": null,
  "env": {
    "DATABASE_URL": "$DATABASE_URL"
  }
}