{
  "version": 2,
  "buildCommand": "echo 'No build needed for static files'",
  "outputDirectory": ".",
  "functions": {
    "api/contact.ts": {
      "runtime": "@vercel/node@3.0.5"
    }
  },
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}